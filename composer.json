{
  "name": "enescakir/leyladan-sonra-web",
  "description": "Leyla'dan Sonra Web Application",
  "version": "v3.1.0",
  "keywords": [
    "leyladan",
    "sonra",
    "social",
    "cancer",
    "medicine"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.3",
    "aws/aws-sdk-php": "^3.130",
    "barryvdh/laravel-snappy": "^0.4.3",
    "doctrine/dbal": "^2.10",
    "enescakir/laravel-helper": "0.2.4",
    "fideloper/proxy": "^4.0",
    "guzzlehttp/guzzle": "^6.5",
    "intervention/image": "^2.5",
    "jenssegers/agent": "^2.6",
    "laravel/framework": "^6.0",
    "laravel/helpers": "^1.1",
    "laravel/horizon": "^3.0",
    "laravel/tinker": "~1.0",
    "laravelcollective/html": "^6.0",
    "maatwebsite/excel": "^3.1",
    "s-ichikawa/laravel-sendgrid-driver": "^2.1",
    "sentry/sentry-laravel": "^1.5",
    "spatie/laravel-analytics": "^3.6",
    "spatie/laravel-backup": "^6.7",
    "spatie/laravel-honeypot": "^1.4",
    "spatie/laravel-medialibrary": "^7.0.0",
    "spatie/laravel-newsletter": "^4.7",
    "spatie/laravel-permission": "^3.0",
    "spatie/laravel-sitemap": "^5.5"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.2",
    "barryvdh/laravel-ide-helper": "^2.6",
    "facade/ignition": "^1.4",
    "fzaninotto/faker": "^1.4",
    "laravel/ui": "^1.1",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^3.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/enescakir/laravel-helper.git"
    }
  ]
}
